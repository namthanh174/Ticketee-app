<%= simple_form_for [project, ticket] do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= image_tag(ticket.attachment_url) if ticket.attachment? %>
  <%= f.input :attachment, as: :file, label: "File" %>
  <%= f.input :attachment_cache, as: :hidden %>
  <%= f.button :submit, class: "btn-primary" %>
<% end %>